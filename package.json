		"tsn_start_dev": "ts-node-dev --project ./tsconfig.node.json src/node/index.ts --respawn",
		"next": "next",
		"next_build": "next build && next export -o public.next",
		"test": "jest",
		"prepare": "husky install"
	},
	"lint-staged": {
		"src/**/*.{js,ts,jsx,tsx}": [
			"eslint --fix --max-warnings=0",
			"git add"
